{
  "extends": "eslint:recommended",
  "settings": {
    "es6": true
  },
  "env": {
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "quotes": "off",
    "eqeqeq": "error",
    "max-depth": ["error", { "max": 3 }],
    "semi": "error",
    "no-var": "error"
  },
  "globals": {
    "require": true,
    "console": true,
    "process": true
  }
}
